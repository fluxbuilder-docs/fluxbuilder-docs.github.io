{
  "name": "docu-notion-sample-site",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "clear-docs": "rimraf ./docs/",
    "pull-test-tagged": "yarn clear-docs && cross-var yarn docu-notion -n $DOCU_NOTION_INTEGRATION_TOKEN -r $DOCU_NOTION_SAMPLE_ROOT_PAGE -m ./docs -i ./static/notion_imgs -p /notion_imgs/ --status-tag test",
    "pull:latest": "yarn clear-docs && cross-var npx @sillsdev/docu-notion -n $DOCU_NOTION_INTEGRATION_TOKEN -r $DOCU_NOTION_SAMPLE_ROOT_PAGE -m ./docs -i ./static/notion_imgs -p /notion_imgs/",
    "//pull": "This uses the specific version of docu-notion in yarn.lock, not necessarily the latest",
    "pull": "yarn clear-docs && cross-var yarn docu-notion -n $DOCU_NOTION_INTEGRATION_TOKEN -r $DOCU_NOTION_SAMPLE_ROOT_PAGE -m ./docs -i ./static/notion_imgs -p /notion_imgs/",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "deploy": "docusaurus deploy",
    "//serve": "Needed, for example, to test the search functionality locally",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations-all": "yarn write-translations --locale zh && yarn write-translations --locale hi && yarn write-translations --locale es && yarn write-translations --locale fr && yarn write-translations --locale ar && yarn write-translations --locale bn && yarn write-translations --locale ru && yarn write-translations --locale pt && yarn write-translations --locale vi",
    "start-zh": "docusaurus start --locale zh",
    "start-vi": "docusaurus start --locale vi",
    "build-all": "docusaurus build"
  },
  "dependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/plugin-google-gtag": "^3.7.0",
    "@docusaurus/preset-classic": "^3.7.0",
    "@easyops-cn/docusaurus-search-local": "^0.40.0",
    "@mdx-js/react": "^3.0.0",
    "@sillsdev/docu-notion": "^0.16.1",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-gif-player": "^0.4.2",
    "react-player": "^2.10.1",
    "rimraf": "^4.1.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/tsconfig": "^3.7.0",
    "@types/node": "^20.11.0",
    "cross-var": "^1.1.0",
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@1.22.19",
  "volta": {
    "node": "18.18.0",
    "yarn": "1.22.19"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
